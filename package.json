{
  "name": "pancake-syntax",
  "displayName": "Pancake Syntax Highlight",
  "description": "Syntax highligter for pancake programming language",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "pancake",
      "aliases": ["Pancake", "pancake"],
      "extensions": [".\uD83E\uDD5E", ".pnk", ".pk"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "pancake",
      "scopeName": "source.pancake",
      "path": "./syntaxes/pancake.tmLanguage.json"
    }]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JunmingZhao42/pancake-vscode-highlight.git"
  },
  "icon": "asset/pancake-logo-small.png",
  "publisher": "JunmingZhao42",
  "license": "MIT"
}